  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "grayscale image shape: (780, 1040)\n"
     ]
    },
    {
     "data": {
      "image/png": "\n",
